{
    "title":            "Default",
    "version":          2,
    "$schema":          "https://schemas.wp.org/trunk/theme.json",
    "patterns":
    [
        "header-default",
        "footer-default",
        "content-default",
        "blog-default",
        "404-default"
    ],
    "customTemplates":
    [
        {
            "name": "custom/blank",
            "title": "Blank",
            "postTypes":
            [
                "page",
                "post"
            ]
        },
        {
            "name": "custom/boxed",
            "title": "Boxed",
            "postTypes":
            [
                "page",
                "post"
            ]
        },
        {
            "name": "custom/full-width",
            "title": "Full width",
            "postTypes":
            [
                "page",
                "post"
            ]
        },
        {
            "name": "custom/wide",
            "title": "Wide",
            "postTypes":
            [
                "page",
                "post"
            ]
        }
    ],
    "templateParts":
    [
        {
            "title": "Header",
            "name": "header",
            "area": "header"
        },
        {
            "title": "Content",
            "name": "content",
            "area": "uncategorized"
        },
        {
            "title": "404",
            "name": "404",
            "area": "uncategorized"
        },
        {
            "title": "Comments",
            "name": "comments",
            "area": "uncategorized"
        },
        {
            "title": "Footer",
            "name": "footer",
            "area": "footer"
        }
    ],
    "settings":
    {
        "layout":
        {
            "wideSize":     "2560px",
            "contentSize":  "1920px"
        },
        "appearanceTools":                  true,
        "useRootPaddingAwareAlignments":    true,
        "typography":
        {
            "fontFamilies":
            [
                {
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
                {
                    "fontFamily": "'Carrois Gothic SC', sans-serif",
                    "name": "Title - Carrois Gothic SC",
                    "slug": "alkamist/carroisgothic-sans",
                    "fontFace":
                    [
                        {
                            "fontFamily": "'Carrois Gothic SC', sans-serif",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Carrois_Gothic_SC/CarroisGothicSC-Regular.ttf"]
                        }
                    ]
                },
                {
                    "name":         "Nunito Sans",
                    "slug":         "alkamist/nunito-sans",
                    "fontFamily":   "'Nunito Sans', sans-serif",
                    "fontFace":
                    [
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "200",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-ExtraLight.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-Light.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-Regular.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "600",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-SemiBold.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-Bold.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "800",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-ExtraBold.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "900",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-Black.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "300",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-Italic.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "200",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-ExtraLightItalic.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "400",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-RegularItalic.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "600",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-SemiBoldItalic.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "700",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-BoldItalic.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "800",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-ExtraBoldItalic.ttf"]
                        },
                        {
                            "fontFamily": "'Nunito Sans', sans-serif",
                            "fontWeight": "900",
                            "fontStyle": "italic",
                            "fontStretch": "normal",
                            "src": ["file:./assets/fonts/Nunito_Sans/Nunito_Sans-BlackItalic.ttf"]
                        }
                    ]
                }
            ],
            "fontSizes":
            [
                {
                    "name": "Small",
                    "slug": "small",
                    "size": "0.9125rem",
                    "fluid":
                    {
                        "max": "0.9125rem",
                        "min": "0.6825rem"
                    }
                },
                {
                    "name": "Medium",
                    "slug": "medium",
                    "size": "1.125rem",
                    "fluid":
                    {
                        "max": "1.125rem",
                        "min": "1rem"
                    }
                },
                {
                    "name": "large",
                    "slug": "large",
                    "size": "1.2919rem",
                    "fluid":
                    {
                        "max": "1.2919rem",
                        "min": "1.1669rem"
                    }
                },
                {
                    "name": "Extra Large",
                    "slug": "x-large",
                    "size": "1.4581rem",
                    "fluid": 
                    {
                        "max": "1.4581rem",
                        "min": "1.3331rem"
                    }
                },
                {
                    "name": "Largest",
                    "slug": "xx-large",
                    "size": "2.225rem",
                    "fluid": 
                    {
                        "max": "2.225rem",
                        "min": "1.625rem"
                    }
                }
            ],
            "fluid": true,
            "dropCap": false,
            "letterSpacing": true,
            "customFontSize": true,
            "fontStyle": true,
            "fontWeight": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true
        },
        "spacing":
        {
            "spacingScale":
            {
                "steps": 0,
                "unit": "rem",
                "operator": "*",
                "increment": 0.5,
                "mediumStep": 3.5
            },
            "spacingSizes":
            [
                {
                    "name": "1",
                    "slug": "1",
                    "size": "clamp(0.225rem, -0.489rem + 1.905vw, 1.225rem)"
                },
                {
                    "name": "2",
                    "slug": "2",
                    "size": "clamp(1.225rem, 0.796rem + 1.143vw, 1.825rem)"
                },
                {
                    "name": "3",
                    "slug": "3",
                    "size": "clamp(1.825rem, 2.254rem + -1.143vw, 1.225rem)"
                },
                {
                    "name": "4",
                    "slug": "4",
                    "size": "clamp(2.4rem, 1.834rem + 1.509vw, 2.8rem)"
                },
                {
                    "name": "5",
                    "slug": "5",
                    "size": "clamp(2.8rem, 1.951rem + 2.264vw, 3.4rem)"
                },
                {
                    "name": "6",
                    "slug": "6",
                    "size": "clamp(3.4rem, 2.834rem + 1.509vw, 3.8rem)"
                }
            ],
            "units": ["px","pt","em","rem","vh","vw","%"],
            "customSpacingSize": true,
            "blockGap": true,
            "padding": true,
            "margin": true
        },
        "color":
        { 
            "link":             true,
            "text":             true,
            "custom":           true,
            "background":       true,
            "customDuotone":    true,
            "customGradient":   true,
            "defaultPalette":   false,
            "defaultDuotone":   true,
            "defaultGradients": true,
            "palette": 
            [
                
                {
                    "name":     "Primary Text",
                    "slug":     "alkamist-primary-text",
                    "color":    "rgb(229, 229, 229)"
                },
                {
                    "name":     "Primary Accent",
                    "slug":     "alkamist-primary-accent",
                    "color":    "rgb(20, 33, 61)"
                },
                {
                    "name":     "Secondary Text",
                    "slug":     "alkamist-secondary-text",
                    "color":    "rgb(245, 245, 245)"
                },
                {
                    "name":     "Secondary Accent",
                    "slug":     "alkamist-secondary-accent",
                    "color":    "rgb(252, 163, 17)"
                },
                {
                    "name":     "Tertiary Text",
                    "slug":     "alkamist-tertiary-text",
                    "color":    "pink"
                },
                {
                    "name":     "Tertiary Accent",
                    "slug":     "alkamist-tertiary-accent",
                    "color":    "rgb(241, 196, 123)"
                },
                
                {
                    "name":     "General Text",
                    "slug":     "alkamist-general-text",
                    "color":    "rgb(0, 0, 0)"
                },
                {
                    "name":     "General Background",
                    "slug":     "alkamist-general-background",
                    "color":    "rgb(255, 255, 255)"
                },
                {
                    "name":     "General Link",
                    "slug":     "alkamist-general-link",
                    "color":    "rgb(70, 110, 195)"
                },
                {
                    "name":     "General Link (Hover)",
                    "slug":     "alkamist-general-link-hover",
                    "color":    "rgb(90, 110, 95)"
                },
                {
                    "name":     "Wash",
                    "slug":     "alkamist-wash",
                    "color":    "rgba(255, 255, 255, 0.2)"
                },
                {
                    "name":     "Tints",
                    "slug":     "alkamist-tints",
                    "color":    "rgba(0, 0, 0, 0.2)"
                }
            ],
            "duotone":
            [
                {
                    "name": "Blues",
                    "slug": "general-to-secondary",					
                    "colors": [ "#000000", "#ffffff" ]
                }
            ],
            "gradients":
            [
                {
                    "name": "Linear red to hot pink",
                    "slug": "alkamist-red-to-hotpink",
                    "gradient": "linear-gradient(160deg, var(--wp--preset--color--alkamist-general), var(--wp--preset--color--alkamist-secondary))"
                }
            ]
        },
        "border":
        {
            "color":    true,
            "style":    true,
            "width":    true,
            "radius":   true
        },
        "blocks":
        {
        },
        "custom":
        {
            "pixelSizes":
            {
                "none":     "0px",
                "tiny":     "1.8px",
                "small":    "5px",
                "medium":   "14px",
                "large":    "18px",
                "larger":   "22px",
                "Huge":     "32px"
            },
            "lineHeights":
            {
                "tiny": "1.2em",
                "small": "1.4em",
                "medium": "1.8em",
                "large": "2em",
                "larger": "2.2em",
                "huge": "2.8em"
            },
            "fontSizes":
            {
                "h1": "2.125rem",
                "h2": "var(--wp--preset--font-size--xx-large)",
                "h3": "var(--wp--preset--font-size--x-large)",
                "h4": "var(--wp--preset--font-size--large)",
                "h5": "var(--wp--preset--font-size--medium)",
                "h6": "var(--wp--preset--font-size--small)",
                "text": "var(--wp--preset--font-size--small)"
            },
            "fontWeights":
            {
                "light": "200",
                "normal": "300",
                "regular": "400",
                "semiBold": "600",
                "bold": "700",
                "bolder": "800",
                "black": "900"
            }
        }
    },
    "styles":
    {
        "color":
        {
            "text": "var(--wp--preset--color--alkamist-general-text)",
            "background": "var(--wp--preset--color--alkamist-general-background)"
        },
        "border":
        {
            "color": "var(--wp--preset--color--alkamist-tints)"
        },
        "spacing": 
        {
            "margin":
            {
                "top":      "var(--wp--custom--pixel-sizes--none)",
                "left":     "var(--wp--custom--pixel-sizes--none)",
                "right":    "var(--wp--custom--pixel-sizes--none)",
                "bottom":   "var(--wp--custom--pixel-sizes--none)"
            },
            "padding":
            {
                "top":      "var(--wp--custom--pixel-sizes--none)",
                "left":     "var(--wp--custom--pixel-sizes--none)",
                "right":    "var(--wp--custom--pixel-sizes--none)",
                "bottom":   "var(--wp--custom--pixel-sizes--none)"
            },
            "blockGap":     "var(--wp--custom--pixel-sizes--none)"
        },
        "typography":
        {
            "fontSize":     "var(--wp--preset--font-size--medium)",
            "fontFamily":   "var(--wp--preset--font-family--alkamist-nunito-sans)",
            "fontWeight":   "var(--wp--custom--font-weights--light)",
            "lineHeight":   "var(--wp--custom--line-heights--large)"
        },
        "elements":
        {
            "h1":
            {
                "typography":
                {
                    "fontSize": "var(--wp--custom--font-sizes--h-1)"
                }
            },
            "h2":
            {
                "typography":
                {
                    "fontSize": "var(--wp--custom--font-sizes--h-2)"
                }
            },
            "h3":
            {
                "typography":
                {
                    "fontSize": "var(--wp--custom--font-sizes--h-3)"
                }
            },
            "h4":
            {
                "typography":
                {
                    "fontSize": "var(--wp--custom--font-sizes--h-4)"
                }
            },
            "h5":
            {
                "typography":
                {
                    "fontSize": "var(--wp--custom--font-sizes--h-5)"
                }
            },
            "h6":
            {
                "typography":
                {
                    "fontSize": "var(--wp--custom--font-sizes--h-6)"
                }
            },
            "link":
            {
                "color":
                {
                    "text": "var(--wp--preset--color--alkamist-general-link)"
                },
                "typography":
                {
                    "fontStyle": "normal",
                    "textDecoration": "underline dashed var(--wp--preset--color--alkamist-general-link)"
                },
                ":hover":
                {
                    "color":
                    {
                        "text": "var(--wp--preset--color--alkamist-general-link-hover)"
                    },
                    "typography":
                    {
                        "textDecoration": "underline dashed var(--wp--preset--color--alkamist-general-link-hover)"
                    }
                }
            },
            "button":
            {
                "color":
                {
                    "text": "var(--wp--preset--color--alkamist-secondary-text)",
                    "background" : "var(--wp--preset--color--alkamist-general-link)"
                },
                "border":
                {
                    "radius":
                    {
                        "topLeft": "var(--wp--custom--pixel-sizes--small)",
                        "topRight": "var(--wp--custom--pixel-sizes--small)",
                        "bottomLeft": "var(--wp--custom--pixel-sizes--small)",
                        "bottomRight": "var(--wp--custom--pixel-sizes--small)"
                    }
                },
                "spacing":
                {
                    "margin":
                    {
                        "top": "var(--wp--custom--pixel-sizes--small)",
                        "left": "var(--wp--custom--pixel-sizes--small)",
                        "right": "var(--wp--custom--pixel-sizes--small)",
                        "bottom": "var(--wp--custom--pixel-sizes--small)"
                    },
                    "padding":
                    {
                        "top": "var(--wp--custom--pixel-sizes--small)",
                        "left": "var(--wp--preset--font-size--small)",
                        "right": "var(--wp--preset--font-size--small)",
                        "bottom": "var(--wp--custom--pixel-sizes--small)"
                    },
                    "blockGap": "var(--wp--custom--pixel-sizes--none)"
                },
                "typography":
                {
                    "fontFamily": "var(--wp--preset--font-family--alkamist-nunito-sans)",
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "var(--wp--custom--font-weights--regular)",
                    "lineHeight": "var(--wp--custom--line-heights--small)",
                    "textTransform": "capitalize"
                },
                ":hover":
                {
                    "color":
                    {
                        "text": "var(--wp--preset--color--alkamist-secondary-text)",
                        "background" : "var(--wp--preset--color--alkamist-general-link-hover)"
                    }
                }
            },
            "cite":
            {
                "spacing":
                {
                    "margin":
                    {
                        "top": "auto",
                        "left": "auto",
                        "right": "auto",
                        "bottom": "auto"
                    },
                    "padding":
                    {
                        "top": "var(--wp--custom--pixel-sizes--none)",
                        "left": "var(--wp--custom--pixel-sizes--none)",
                        "right": "var(--wp--custom--pixel-sizes--none)",
                        "bottom": "var(--wp--custom--pixel-sizes--none)"
                    },
                    "blockGap": "var(--wp--custom--pixel-sizes--none)"
                },
                "typography":
                {
                    "fontFamily": "var(--wp--preset--font-family--alkamist-nunito-sans)",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontStyle": "italic",
                    "fontWeight": "var(--wp--custom--font-weights--regular)",
                    "lineHeight": "var(--wp--custom--line-heights--small)",
                    "textTransform": "capitalize"
                }
            },
            "caption":
            {
                "color":
                {
                    "background": "var(--wp--preset--color--alkamist-tints)"
                },
                "spacing":
                {
                    "padding":
                    {
                        "top": "var(--wp--custom--pixel-sizes--small)",
                        "left": "var(--wp--custom--pixel-sizes--small)",
                        "right": "var(--wp--custom--pixel-sizes--small)",
                        "bottom": "var(--wp--custom--pixel-sizes--small)"
                    },
                    "margin":
                    {
                        "top": "var(--wp--custom--pixel-sizes--small)",
                        "left": "var(--wp--custom--pixel-sizes--none)",
                        "right": "var(--wp--custom--pixel-sizes--none)",
                        "bottom": "var(--wp--custom--pixel-sizes--medium)"
                    },
                    "blockGap": "var(--wp--custom--pixel-sizes--none)"
                },
                "border":
                {
                    "radius":
                    {
                        "topLeft": "var(--wp--custom--pixel-sizes--small)",
                        "topRight": "var(--wp--custom--pixel-sizes--small)",
                        "bottomLeft": "var(--wp--custom--pixel-sizes--small)",
                        "bottomRight": "var(--wp--custom--pixel-sizes--small)"
                    }
                },
               "typography":
               {
                    "fontFamily": "var(--wp--preset--font-family--alkamist-nunito-sans)",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontStyle": "italic",
                    "fontWeight": "var(--wp--custom--font-weights--light)",
                    "lineHeight": "var(--wp--custom--line-heights--small)",
                    "textTransform": "full-size-kana"
               }
            },
            "heading":
            {
                "color":
                {
                    "text": "var(--wp--preset--color--alkamist-general-text)",
                    "background": "transparent"
                },
                "typography":
                {
                    "fontFamily": "var(--wp--preset--font-family--alkamist-nunito-sans)",
                    "lineHeight":   "var(--wp--custom--line-heights--medium)",
                    "fontWeight":   "var(--wp--custom--font-weights--black)"
                }
            }
        },
        "blocks":
        {
            "core/group":
            {
                "spacing":
                {
                    "padding":
                    {
                        "top": "var(--wp--custom--pixel-sizes--small)",
                        "left": "var(--wp--custom--pixel-sizes--small)",
                        "right": "var(--wp--custom--pixel-sizes--small)",
                        "bottom": "var(--wp--custom--pixel-sizes--small)"
                    },
                    "margin":
                    {
                        "top": "var(--wp--custom--pixel-sizes--none)",
                        "left": "var(--wp--custom--pixel-sizes--none)",
                        "right": "var(--wp--custom--pixel-sizes--none)",
                        "bottom": "var(--wp--custom--pixel-sizes--none)"
                    },
                    "blockGap": "var(--wp--custom--pixel-sizes--none)"
                }
            },
            "core/row":
            {
                "spacing":
                {
                    "padding":
                    {
                        "top": "var(--wp--custom--pixel-sizes--small)",
                        "left": "var(--wp--custom--pixel-sizes--small)",
                        "right": "var(--wp--custom--pixel-sizes--small)",
                        "bottom": "var(--wp--custom--pixel-sizes--small)"
                    },
                    "margin":
                    {
                        "top": "var(--wp--custom--pixel-sizes--none)",
                        "left": "var(--wp--custom--pixel-sizes--none)",
                        "right": "var(--wp--custom--pixel-sizes--none)",
                        "bottom": "var(--wp--custom--pixel-sizes--none)"
                    },
                    "blockGap": "var(--wp--custom--pixel-sizes--none)"
                }
            },
            "core/stack":
            {
                "spacing":
                {
                    "margin":
                    {
                        "top": "var(--wp--custom--pixel-sizes--none)",
                        "bottom": "var(--wp--custom--pixel-sizes--none)"
                    }
                }
            },
            "core/columns":
            {
                "spacing":
                {
                    "margin":
                    {
                        "top": "var(--wp--custom--pixel-sizes--medium)",
                        "left": "var(--wp--custom--pixel-sizes--medium)",
                        "right": "var(--wp--custom--pixel-sizes--medium)",
                        "bottom": "var(--wp--custom--pixel-sizes--medium)"
                    },
